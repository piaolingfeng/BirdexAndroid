<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_inventory_willin_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/inventory_item_text"
        android:textColor="@color/inventory_item_gray"/>
    <TextView
        android:id="@+id/tv_inventory_willin_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/inventory_item_text"
        android:textColor="@color/inventory_item_blue"/>
</LinearLayout>